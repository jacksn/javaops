<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true" scanPeriod="30 seconds">

    <property name="CONSOLE_LOG_PATTERN" value="%d{HH:mm:ss.SSS} %-5level %class{36}.%M:%L - %msg%n"/>
    <property name="FILE_LOG_PATTERN" value="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %class{36}.%M:%L - %msg%n"/>

    <include resource="org/springframework/boot/logging/logback/base.xml"/>
    <!-- <logger name="org.springframework.web" level="DEBUG"/> -->
    <!--<logger name="org.springframework.web.servlet.resource.PathResourceResolver" level="trace"/>-->
    <logger name="org.springframework.web.servlet.mvc.method.annotation.ExceptionHandlerExceptionResolver" level="info"/>

<!--
    <logger name="org.springframework.security.web" level="debug"/>
    <logger name="org.springframework.web" level="debug"/>
-->

    <logger name="ru.javaops" level="debug"/>

</configuration>
